Options +FollowSymLinks

RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !/public
RewriteCond %{REQUEST_URI} ^(.*)\.(js|css|xml|xsl|png|jpg|gif|ico|swf|txt|3ds)
RewriteRule ^(.*)\.([A-Za-z0-9]{0,3})$ public/$1\.$2 [L]

RewriteCond %{REQUEST_FILENAME} !/public
RewriteRule ^.*$ public/lx-dispatch.php [QSA,L]
